{
  "version": "0.1",
  "name": "drageisa",
  "description": "Informações para o atendimento via WhatsApp do consultório da Dra. Geisa.",
  "epilog": "Utilitário da Dra. Geisa desenvolvido por Daniel Collier.",
  "params": [
    {
      "arg_short": "-m",
      "arg_extended": "--message",
      "arg_help": "Número da messagem",
      "default": "all"
    },
    {
      "arg_short": "-p",
      "arg_extended": "--paciente",
      "arg_help": "Nome da paciente"
    },
    {
      "arg_short": "-c",
      "arg_extended": "--consulta",
      "arg_help": "Orientações da consulta"
    },
    {
      "arg_short": "-i",
      "arg_extended": "--info",
      "arg_help": "Informações gerais"
    },
    {
      "arg_short": "-d",
      "arg_extended": "--dia",
      "arg_help": "Dia da consulta"
    },
    {
      "arg_short": "-t",
      "arg_extended": "--hora",
      "arg_help": "Hora da consulta"
    }
  ]
}
